<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:padding="5dp"
    >

    <TextView
        android:id="@+id/namel"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="name"
        android:textSize="25dp"
        android:textColor="#000"
        android:layout_margin="5dp"
        />
    <TextView
        android:id="@+id/agel"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="age"
        android:textSize="25dp"
        android:textColor="#000"
        android:layout_margin="5dp"
        />
    <TextView
        android:id="@+id/regl"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:text="reg"
    android:textSize="25dp"
    android:textColor="#000"
    android:layout_margin="5dp"
    /><TextView
    android:id="@+id/dobl"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:text="dob"
    android:textSize="25dp"
    android:textColor="#000"
    android:layout_margin="5dp"
    /><TextView
    android:id="@+id/coursel"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:text="course"
    android:textSize="25dp"
    android:textColor="#000"
    android:layout_margin="5dp"
    /><TextView
    android:id="@+id/email"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:text="email"
    android:textSize="25dp"
    android:textColor="#000"
    android:layout_margin="5dp"
    /><TextView
    android:id="@+id/placel"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:text="place"
    android:textSize="25dp"
    android:textColor="#000"
    android:layout_margin="5dp"
    /><TextView
    android:id="@+id/bloodl"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:text="blood"
    android:textSize="25dp"
    android:textColor="#000"
    android:layout_margin="5dp"
    />

</LinearLayout>